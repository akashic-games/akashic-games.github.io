		"subweapon": {
			"type": "image",
			"width": 64,
			"height": 32,
			"path": "image/subweapon.png"
		},
		"yrot": {
			"type": "image",
			"width": 64,
			"height": 32,
			"path": "image/yrot.png"
		},
		"Particle": {
			"type": "script",
			"path": "script/Particle.js",
			"global": true
		},
		"play": {
			"type": "image",
			"width": 64,
			"height": 32,
			"path": "image/play.png"
		},
