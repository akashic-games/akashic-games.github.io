{
	"width": 640,
	"height": 480,
	"fps": 30,
	"title": "akashic-animation",
	"description": "拡張ライブラリ[akashic-animation](https://github.com/akashic-games/akashic-animation)でアニメーションを制御することができます。",
	"main": "./script/main.js",
	"assets": {
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"demo": {
			"type": "script",
			"path": "script/demo.js",
			"global": true
		},
		"an_anime_1": {
			"type": "text",
			"path": "text/an_anime_1.asaan"
		},
		"stickman": {
			"type": "image",
			"width": 256,
			"height": 256,
			"path": "image/stickman.png"
		},
		"bn_stickman": {
			"type": "text",
			"path": "text/bn_stickman.asabn"
		},
		"pj_stickman": {
			"type": "text",
			"path": "text/pj_stickman.asapj",
			"global": true
		},
		"sk_stickman": {
			"type": "text",
			"path": "text/sk_stickman.asask"
		},
		"an_anime_1_bezier": {
			"type": "text",
			"path": "text/an_anime_1_bezier.asaan"
		},
		"an_anime_1_liner": {
			"type": "text",
			"path": "text/an_anime_1_liner.asaan"
		},
		"loop": {
			"type": "image",
			"width": 64,
			"height": 32,
			"path": "image/loop.png"
		},
		"particle": {
			"type": "image",
			"width": 64,
			"height": 32,
			"path": "image/particle.png"
