{
	"width": 1280,
	"height": 720,
	"fps": 30,
	"main": "./script/main.js",
	"title": "ソフトウェアキーボード",
	"description": "[ソフトウェアキーボード](https://github.com/akashic-contents/software-keyboard-sample)のサンプルです。画面にキーボードを出し文字を入力できるようになります。",
	"assets": {
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"backSpaceKey": {
			"type": "image",
			"width": 72,
			"height": 72,
			"path": "image/backSpaceKey.png"
		},
		"inputtingLabelBack": {
			"type": "image",
			"width": 1264,
			"height": 112,
			"path": "image/inputtingLabelBack.png"
		},
		"define": {
			"type": "script",
			"path": "script/define.js",
			"global": true
		},
		"Keyboard": {
			"type": "script",
			"path": "script/Keyboard.js",
			"global": true
		},
		"KeyboardParameterObject": {
			"type": "script",
			"path": "script/KeyboardParameterObject.js",
			"global": true
		},
		"MultiKeyboard": {
			"type": "script",
			"path": "script/MultiKeyboard.js",
			"global": true
		},
		"toAlphaLetters": {
			"type": "image",
			"width": 180,
			"height": 72,
			"path": "image/toAlphaLetters.png"
		},
		"toKanaLetters": {
			"type": "image",
			"width": 180,
			"height": 72,
			"path": "image/toKanaLetters.png"
		},
		"toSymLetters": {
			"type": "image",
			"width": 180,
			"height": 72,
			"path": "image/toSymLetters.png"
		},
		"close": {
			"type": "image",
			"width": 256,
			"height": 88,
			"path": "image/close.png"
		},
		"open": {
			"type": "image",
			"width": 256,
			"height": 88,
			"path": "image/open.png"
		},
		"notosansFont": {
			"type": "image",
			"width": 720,
			"height": 1008,
			"path": "image/notosansFont.png"
		},
		"notosansGlyph": {
			"type": "text",
			"path": "text/notosansGlyph.json"
		},
		"SpriteReader": {
			"type": "script",
			"path": "script/SpriteReader.js",
			"global": true
		},
		"img_key_alpha": {
			"type": "image",
			"width": 616,
			"height": 533,
			"path": "image/img_key_alpha.png"
		},
		"img_key_kana": {
			"type": "image",
			"width": 586,
			"height": 590,
			"path": "image/img_key_kana.png"
		},
		"img_key_sym": {
			"type": "image",
			"width": 558,
			"height": 516,
			"path": "image/img_key_sym.png"
		},
		"hint_key_alpha": {
			"type": "text",
			"path": "text/hint_key_alpha.json"
		},
		"hint_key_kana": {
			"type": "text",
			"path": "text/hint_key_kana.json"
		},
		"hint_key_sym": {
			"type": "text",
			"path": "text/hint_key_sym.json"
		}
	},
	"environment": {
		"sandbox-runtime": "3"
	},
	"globalScripts": [
		"node_modules/@akashic-extension/akashic-label/lib/DefaultRubyParser.js",
		"node_modules/@akashic-extension/akashic-label/lib/FragmentDrawInfo.js",
		"node_modules/@akashic-extension/akashic-label/lib/Label.js",
		"node_modules/@akashic-extension/akashic-label/lib/RubyParser.js",
		"node_modules/@akashic-extension/akashic-label/lib/index.js",
		"node_modules/@akashic-extension/akashic-timeline/lib/ActionType.js",
		"node_modules/@akashic-extension/akashic-timeline/lib/Easing.js",
		"node_modules/@akashic-extension/akashic-timeline/lib/Timeline.js",
		"node_modules/@akashic-extension/akashic-timeline/lib/Tween.js",
		"node_modules/@akashic-extension/akashic-timeline/lib/index.js"
	],
	"moduleMainScripts": {
		"@akashic-extension/akashic-label": "node_modules/@akashic-extension/akashic-label/lib/index.js",
		"@akashic-extension/akashic-timeline": "node_modules/@akashic-extension/akashic-timeline/lib/index.js"
	}
}