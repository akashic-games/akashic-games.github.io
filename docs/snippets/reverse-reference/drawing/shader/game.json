{
  "width": 320,
  "height": 320,
  "fps": 60,
  "main": "./main.js",
  "renderers": ["webgl"],
  "assets": {
    "main": {
      "type": "script",
      "path": "main.js",
      "global": true
    },
    "player": {
      "type": "image",
      "width": 63,
      "height": 53,
      "path": "image/player.png"
    },
    "monochrome": {
      "type": "text",
      "path": "text/monochrome.glsl"
    }
  },
  "environment": {
    "sandbox-runtime": "3"
  }
}
