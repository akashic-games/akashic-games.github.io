{
	"width": 320,
	"height": 320,
	"fps": 30,
	"title": "akashic-label",
	"description": "拡張ライブラリ[akashic-label](https://github.com/akashic-games/akashic-label)でルビや改行を含むテキストを描画することが出来ます。",
	"main": "./script/main.js",
	"assets": {
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"scene1": {
			"type": "script",
			"path": "script/scene1.js",
			"global": true
		},
		"scene2": {
			"type": "script",
			"path": "script/scene2.js",
			"global": true
		},
		"scene3": {
			"type": "script",
			"path": "script/scene3.js",
			"global": true
		},
		"scene4": {
			"type": "script",
			"path": "script/scene4.js",
			"global": true
		},
		"scene5": {
			"type": "script",
			"path": "script/scene5.js",
			"global": true
		},
		"scene6": {
			"type": "script",
			"path": "script/scene6.js",
			"global": true
		},
		"bmpfont": {
			"type": "image",
			"width": 512,
			"height": 459,
			"path": "image/bmpfont.png"
		},
		"bmpfont-glyph": {
			"type": "text",
			"path": "text/bmpfont-glyph.json"
		},
		"mplus": {
			"type": "image",
			"width": 2048,
			"height": 702,
			"path": "image/mplus.png"
		},
		"mplus-glyph": {
			"type": "text",
			"path": "text/mplus-glyph.json"
		}
	},
	"moduleMainScripts": {
		"@akashic-extension/akashic-label": "node_modules/@akashic-extension/akashic-label/lib/index.js"
	},
	"globalScripts": [
		"node_modules/@akashic-extension/akashic-label/lib/Label.js",
		"node_modules/@akashic-extension/akashic-label/lib/DefaultRubyParser.js",
		"node_modules/@akashic-extension/akashic-label/lib/RubyParser.js",
		"node_modules/@akashic-extension/akashic-label/lib/FragmentDrawInfo.js",
		"node_modules/@akashic-extension/akashic-label/lib/index.js"
	],
	"environment": {
		"sandbox-runtime": "3"
	}
}
